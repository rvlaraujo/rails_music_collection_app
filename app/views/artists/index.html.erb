<p id="notice"><%= notice %></p>

<h1>Artists#index</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Twitter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.id %></td>
        <td><%= artist.name %></td>
        <td><%= artist.twitter %></td>
        <td><%= link_to 'Show', artist_path(artist.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
